    { name: "Milk", price: 50, category: "Dairy" },
    { name: "Rice", price: 60, category: "Grains" }
];

function displayProducts() {
    products.forEach(product => {
        console.log(
            product.name + 
            " - â‚¹" + product.price + 
            " (" + product.category + ")"
        );
    });
}

